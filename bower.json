{
  "name": "static-starter",
  "version": "0.1.0",
  "homepage": "http://reyhoun.com",
  "authors": [
    "Reyhoun Team <info@reyhoun.com>"
  ],
  "license": "MIT",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components"
  ],
  "dependencies": {
    "foundation": "^5.4.7",
    "sass-direction-controller": "^1.0.2",
    "fontawesome": "^4.2.0"
  },
  "overrides": {
      "foundation": {
		"dependencies": null,
		"main": ["js/**/*", "scss/**/*"]
      },
      "fontawesome": {
		"main": ["scss/*", "fonts/*"]
      }
  }
}